{
  "Variable Declaration": {
    "prefix": "var",
    "body": [
      "${1:type} | ${2:name} -> ${3:value}"
    ],
    "description": "Variable declaration"
  },
  "Integer Variable": {
    "prefix": "int",
    "body": [
      "int | ${1:name} -> ${2:0}"
    ],
    "description": "Integer variable"
  },
  "String Variable": {
    "prefix": "str",
    "body": [
      "str | ${1:name} -> \"${2:value}\""
    ],
    "description": "String variable"
  },
  "List Variable": {
    "prefix": "list",
    "body": [
      "list | ${1:name} -> [${2:}]"
    ],
    "description": "List variable"
  },
  "Dictionary Variable": {
    "prefix": "dict",
    "body": [
      "dict | ${1:name} -> {${2:}}"
    ],
    "description": "Dictionary variable"
  },
  "Function": {
    "prefix": "fn",
    "body": [
      "fn | ${1:name} (${2:params}) > {",
      "  ${3:# function body}",
      "  ${4:return_value}",
      "}"
    ],
    "description": "Function definition"
  },
  "Function with Return Type": {
    "prefix": "fnt",
    "body": [
      "fn ${1:return_type} | ${2:name} (${3:params}) > {",
      "  ${4:# function body}",
      "  ${5:return_value}",
      "}"
    ],
    "description": "Function with return type"
  },
  "Lambda Function": {
    "prefix": "lambda",
    "body": [
      "fn | ${1:name} -> fn ${2:param} > ${3:expression}"
    ],
    "description": "Lambda function"
  },
  "If Statement": {
    "prefix": "if",
    "body": [
      "if ${1:condition} {",
      "  ${2:# code}",
      "}"
    ],
    "description": "If statement"
  },
  "If-Else Statement": {
    "prefix": "ife",
    "body": [
      "if ${1:condition} {",
      "  ${2:# code}",
      "} else {",
      "  ${3:# code}",
      "}"
    ],
    "description": "If-else statement"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for ${1:item} in ${2:iterable} {",
      "  ${3:# code}",
      "}"
    ],
    "description": "For loop"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while ${1:condition} {",
      "  ${2:# code}",
      "}"
    ],
    "description": "While loop"
  },
  "Range Loop": {
    "prefix": "range",
    "body": [
      "${1:i} in ${2:0}..${3:10} : ${4:out(i)}"
    ],
    "description": "Range loop"
  },
  "Class": {
    "prefix": "class",
    "body": [
      "class | ${1:ClassName} {",
      "  ${2:# properties}",
      "  ",
      "  fn | init (${3:params}) > {",
      "    ${4:# initialization}",
      "  }",
      "  ",
      "  fn | ${5:method} (${6:params}) > {",
      "    ${7:# method body}",
      "  }",
      "}"
    ],
    "description": "Class definition"
  },
  "Trait": {
    "prefix": "trait",
    "body": [
      "trait | ${1:TraitName} {",
      "  fn | ${2:method} (${3:params}) > {",
      "    ${4:# default implementation}",
      "  }",
      "}"
    ],
    "description": "Trait definition"
  },
  "Module": {
    "prefix": "module",
    "body": [
      "module | ${1:module_name} {",
      "  ${2:# module contents}",
      "}"
    ],
    "description": "Module definition"
  },
  "Import": {
    "prefix": "import",
    "body": [
      "import ${1:module.name}"
    ],
    "description": "Import statement"
  },
  "Async Function": {
    "prefix": "async",
    "body": [
      "async fn | ${1:name} (${2:params}) > {",
      "  ${3:# async code}",
      "  ${4:return_value}",
      "}"
    ],
    "description": "Async function"
  },
  "Await": {
    "prefix": "await",
    "body": [
      "${1:result} <- await ${2:async_function}(${3:args})"
    ],
    "description": "Await expression"
  },
  "Match Statement": {
    "prefix": "match",
    "body": [
      "match ${1:value} {",
      "  ${2:pattern1} > ${3:result1}",
      "  ${4:pattern2} > ${5:result2}",
      "  _ > ${6:default}",
      "}"
    ],
    "description": "Match statement"
  },
  "Decorator": {
    "prefix": "deco",
    "body": [
      "@${1:decorator}",
      "fn | ${2:name} (${3:params}) > {",
      "  ${4:# function body}",
      "}"
    ],
    "description": "Decorated function"
  },
  "Memo Decorator": {
    "prefix": "memo",
    "body": [
      "@memo",
      "fn | ${1:name} (${2:params}) > {",
      "  ${3:# expensive computation}",
      "}"
    ],
    "description": "Memoized function"
  },
  "Once Decorator": {
    "prefix": "once",
    "body": [
      "@once",
      "fn | ${1:name} > {",
      "  ${2:# runs only once}",
      "}"
    ],
    "description": "Once decorator"
  },
  "Print": {
    "prefix": "out",
    "body": [
      "out(${1:value})"
    ],
    "description": "Print output"
  },
  "Pipeline": {
    "prefix": "pipe",
    "body": [
      "${1:value} |> ${2:function1} |> ${3:function2}"
    ],
    "description": "Pipeline operator"
  },
  "Broadcast": {
    "prefix": "broadcast",
    "body": [
      "${1:function}.(${2:list})"
    ],
    "description": "Broadcast operator"
  },
  "List Comprehension": {
    "prefix": "listcomp",
    "body": [
      "[${1:expression} for ${2:item} in ${3:iterable}]"
    ],
    "description": "List comprehension"
  },
  "Dict Comprehension": {
    "prefix": "dictcomp",
    "body": [
      "{${1:key}: ${2:value} for ${3:item} in ${4:iterable}}"
    ],
    "description": "Dictionary comprehension"
  },
  "Try-Catch": {
    "prefix": "try",
    "body": [
      "try {",
      "  ${1:# code that might fail}",
      "} catch ${2:error} {",
      "  ${3:# error handling}",
      "}"
    ],
    "description": "Try-catch block"
  },
  "Defer": {
    "prefix": "defer",
    "body": [
      "defer {",
      "  ${1:# cleanup code}",
      "}"
    ],
    "description": "Defer block"
  },
  "Window Loop": {
    "prefix": "window",
    "body": [
      "window ${1:slice} in ${2:list} {",
      "  ${3:# process window}",
      "}"
    ],
    "description": "Window loop"
  },
  "Group By": {
    "prefix": "groupby",
    "body": [
      "dict | ${1:groups} -> group_by(${2:list}, fn | ${3:item} > ${4:key})"
    ],
    "description": "Group by function"
  },
  "Partition": {
    "prefix": "partition",
    "body": [
      "tuple | ${1:result} -> partition(${2:list}, fn | ${3:item} > ${4:condition})"
    ],
    "description": "Partition function"
  },
  "Counter": {
    "prefix": "counter",
    "body": [
      "counter | ${1:counts} -> ${2:list}"
    ],
    "description": "Counter from list"
  },
  "Grid": {
    "prefix": "grid",
    "body": [
      "grid | ${1:g} -> grid(${2:rows}, ${3:cols}, ${4:default})"
    ],
    "description": "Create grid"
  },
  "File Read": {
    "prefix": "read",
    "body": [
      "str | ${1:content} -> read(\"${2:filename}\")"
    ],
    "description": "Read file"
  },
  "File Write": {
    "prefix": "write",
    "body": [
      "write(\"${1:filename}\", ${2:content})"
    ],
    "description": "Write file"
  },
  "Main Function": {
    "prefix": "main",
    "body": [
      "fn | main > {",
      "  ${1:# program entry point}",
      "}",
      "",
      "main()"
    ],
    "description": "Main function"
  },
  "Test Function": {
    "prefix": "test",
    "body": [
      "@test",
      "fn | test_${1:name} > {",
      "  ${2:# test code}",
      "  assert ${3:condition}",
      "}"
    ],
    "description": "Test function"
  },
  "Jolt Init": {
    "prefix": "jolt",
    "body": [
      "# Run: j jolt init ${1:project-name}"
    ],
    "description": "Jolt project init command"
  }
}
