{
  "name": "J Light",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#000000",
    "editorLineNumber.foreground": "#237893",
    "editorCursor.foreground": "#000000"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#008000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.control", "keyword.other"],
      "settings": {
        "foreground": "#AF00DB"
      }
    },
    {
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#267F99"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#A31515"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#098658"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#795E26"
      }
    },
    {
      "scope": ["support.function.builtin"],
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": ["entity.name.function.decorator"],
      "settings": {
        "foreground": "#267F99",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#0000FF"
      }
    }
  ]
}
